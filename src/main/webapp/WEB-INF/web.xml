<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
		version = "4.0">
		
		<servlet>
			<servlet-name>Customer</servlet-name>
			<servlet-class>Reservation.CustomerServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>Customer</servlet-name>
			<url-pattern>/customer</url-pattern>
		</servlet-mapping>
		
		
		
		<servlet>
			<servlet-name>InsertResServlet</servlet-name>
			<servlet-class>Reservation.InsertResServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>InsertResServlet</servlet-name>
			<url-pattern>/insert</url-pattern>
		</servlet-mapping>
		
		
		
		<servlet>
			<servlet-name>UpdateResServlet</servlet-name>
			<servlet-class>Reservation.UpdateResServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>UpdateResServlet</servlet-name>
			<url-pattern>/update</url-pattern>
		</servlet-mapping>
		
		
		<servlet>
			<servlet-name>DeleteResServlet</servlet-name>
			<servlet-class>Reservation.DeleteResServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>DeleteResServlet</servlet-name>
			<url-pattern>/delete</url-pattern>
		</servlet-mapping>
		
</web-app>